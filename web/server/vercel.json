{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "installCommand": "corepack enable pnpm && corepack prepare pnpm@10.20.0 --activate && pnpm install",
  "buildCommand": "pnpm build"
}
