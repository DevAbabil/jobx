{
  "name": "jobx",
  "version": "1.0.0",
  "description": "AI-powered CLI tool to automate job applications, generate personalized emails, and track applications using Google Sheets.",
  "main": "./dist/index.js",
  "bin": {
    "jobx": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx --watch ./src/index.ts",
    "start": "node ./dist/index.js",
    "build": "sh ./scripts/build.sh",
    "publish:latest": "sh ./scripts/publish.sh",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint --write .",
    "lint:check": "biome lint .",
    "check": "biome check --write .",
    "check:ci": "biome check .",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevAbabil/jobx.git"
  },
  "keywords": [
    "job",
    "jobx",
    "cli",
    "automation",
    "ai",
    "typescript",
    "job-application"
  ],
  "author": "Md Ababil Hossain <ababil.webdev@gmail.com>",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/DevAbabil/jobx/issues"
  },
  "homepage": "https://github.com/DevAbabil/jobx#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@types/node": "^24.9.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
